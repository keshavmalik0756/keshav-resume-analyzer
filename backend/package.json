{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "build": "cd ../frontend && npm run build && cd ../backend && npx rimraf dist && mkdir dist && xcopy ..\\frontend\\dist dist /E /I /H /Y",
    "postinstall": "npm run build",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "node-cache": "^5.1.2",
    "pdf-parse": "^1.1.1",
    "pdfkit": "^0.17.2",
    "rimraf": "^6.0.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "eventsource": "^4.0.0",
    "jest": "^30.0.5",
    "supertest": "^7.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "middleware/**/*.js",
      "routes/**/*.js",
      "services/**/*.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}